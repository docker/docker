#!/usr/bin/env bash
# Entrypoint for jenkins janky CI build
set -eu -o pipefail

hack/validate/default
hack/test/unit

JENKINS_URL=https://jenkins.dockerproject.org \
bash <(curl -s https://codecov.io/bash) \
    -f coverage.txt \
    -C "$GIT_SHA1" -Z

hack/make.sh \
	binary-daemon \
	dynbinary \
	test-docker-py \
	test-integration \
	cross
