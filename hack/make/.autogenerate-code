#!/bin/bash

cat > dockerversion/details.go <<EOF
// AUTOGENERATED FILE; see hack/make/.autogenerate-code
package dockerversion

func init() {
	GITCOMMIT = "$GITCOMMIT"
	VERSION = "$VERSION"
}
EOF

cat > dockerversion/static.go <<EOF
// AUTOGENERATED FILE; see hack/make/.autogenerate-code
package dockerversion

func init() {
	IAMSTATIC = ${IAMSTATIC:-true}
}
EOF

cat > dockerversion/init.go <<EOF
// AUTOGENERATED FILE; see hack/make/.autogenerate-code
package dockerversion

func init() {
	INITSHA1 = "$DOCKER_INITSHA1"
	INITPATH = "$DOCKER_INITPATH"
}
EOF
